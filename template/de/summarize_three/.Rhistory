gid<-Reduce('intersect', lapply(stat, rownames));
# Gene annotation
anno<-lapply(res, function(res) res$inputs$anno);
cnm <- Reduce('intersect', lapply(anno, colnames));
anno[[1]] <- anno[[1]][, colnames(anno[[1]]) %in% cnm, drop = FALSE];
anno[[2]] <- anno[[2]][, colnames(anno[[1]]), drop=FALSE];
anno[[3]] <- anno[[3]][, colnames(anno[[1]]), drop=FALSE];
anno.all <- lapply(anno, function(a) a[rownames(a) %in% gid, , drop=FALSE]);
anno <- anno.all[[1]];
anno <- rbind(anno, anno.all[[2]][setdiff(rownames(anno.all[[2]]), rownames(anno)), , drop=FALSE]);
anno <- rbind(anno, anno.all[[3]][setdiff(rownames(anno.all[[3]]), rownames(anno)), , drop=FALSE]);
anno <- anno[gid, , drop=FALSE];
rownames(anno) <- gid;
anno[is.na(anno[, 1]), 1] <- rownames(anno)[is.na(anno[, 1])];
